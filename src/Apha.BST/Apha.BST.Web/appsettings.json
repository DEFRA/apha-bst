{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "AwsLogging": {
        "LogGroupName": ""
    },
    "LogsPath": "Logs",
    "ConnectionStrings": {
        "BSTConnectionString": "Data Source=<<servername>>;Initial Catalog=<<dbname>>;Integrated Security=True;Encrypt=False;Connection Timeout=30"

    },
    "AzureAd": {
        "Instance": "https://login.microsoftonline.com/",
        "Domain": "<domain_name>.com",
        "ClientId": "<client_Id>",
        "TenantId": "<tenant_Id>",
        "ClientSecret": "<client_Secret>",
        "ClientCertificates": [
        ],
        "CallbackPath": "/signin-oidc"
    },
    "ReportSettings": {
        "TemplateFilePath": "wwwroot/Reports/BST.xlsx"
    },
    "ExceptionTypes": {
        "General": "BST.GENERAL_EXCEPTION",
        "Sql": "BST.SQL_EXCEPTION",
        "Authentication": "BST.AUTHENTICATION_EXCEPTION"
    }
}
